declare MODULE=kdepim/runtime
declare REPO=KDE/kdepim-runtime

create repository ${REPO}
end repository

##############
# Some stuff we want to ignore
##############
include kdepim-common-ignore-rules

##############
# Main rules
##############
match /trunk/KDE/kdepim/runtime/
    repository ${REPO}
    branch master
#    min revision 1061321
end match
