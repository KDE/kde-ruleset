##############
# Branches...
##############
# Non merged branches
match /branches/work/(akonadi-ports)/kdepim/(akonadi)/
    repository ${REPO}
    branch \1
    prefix \2
end match
#match /branches/work/(akonadi-ports)/kdepim/runtime/$
#    action recurse
#end match
match /branches/work/(akonadi-ports)/kdepim/runtime/
    repository ${REPO}
    branch \1
end match

#match /branches/(work)/(arzie_akonadi)/$
#    action recurse
#end match
match /branches/(work)/(arzie_akonadi)/
    repository ${REPO}
    branch \1/\2
end match

match /branches/kdepim/(soc)/kdepim/akonadi/
    repository ${REPO}
    branch \1
    prefix akonadi/
end match
