create repository kregexpeditor
end repository

include ../../common-kde-ignores

# KRegExpEditor
#

match /(branches|tags)/KDE_[^/]+/(kdeutils/)?$
  action recurse
end match

match /(branches|tags)/KDE/[^/]+/(kdeutils/)?$
  action recurse
end match

declare REPO=kregexpeditor
declare MODULE=kdeutils/kregexpeditor
include ../../common-kde-tags-rules


# general rules for version branches
match /branches/KDE_(\d+)_(\d+)_BRANCH/kdeutils/kregexpeditor/
  repository kregexpeditor
  branch KDE/\1.\2
end match

match /branches/KDE/(\d+)\.(\d+)/${MODULE}/
  repository kregexpeditor
  branch KDE/\1.\2
end match

# KDE/3.0 branch

match /branches/KDE/3.0/kdeutils/doc/KRegExpEditor/
  repository kregexpeditor
  branch KDE/3.0
  min revision 409209
  prefix doc/
end match

match /branches/KDE_3_0_BRANCH/kdeutils/doc/KRegExpEditor/
  repository kregexpeditor
  branch KDE/3.0
  min revision 145201
  prefix doc/
end match

# END - KDE/3.0

