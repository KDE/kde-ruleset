create repository kregexpeditor
end repository

include ../../common-kde-ignores

# KRegExpEditor
#

match /(branches|tags)/KDE_[^/]+/(kdeutils/)?$
  action recurse
end match

match /(branches|tags)/KDE/[^/]+/(kdeutils/)?$
  action recurse
end match

declare REPO=kregexpeditor
declare MODULE=kdeutils/kregexpeditor
include ../../common-kde-tags-rules


# general rules for version branches
match /branches/KDE_(\d+)_(\d+)_BRANCH/kdeutils/kregexpeditor/
  repository kregexpeditor
  branch KDE/\1.\2
end match

match /branches/KDE/(\d+)\.(\d+)/${MODULE}/
  repository kregexpeditor
  branch KDE/\1.\2
end match

# documentation
# (note: there were no doc changes in KDE_x_y_BRANCH branches)

match /branches/KDE/(\d+)\.(\d+)/kdeutils/doc/KRegExpEditor/
  repository kregexpeditor
  branch KDE/\1.\2
  prefix doc/
end match

# main history

match /trunk/kdeutils/kregexpeditor/
  repository kregexpeditor
  branch master
  max revision 409202
  action recurse
end match

match /trunk/KDE/kdeutils/kregexpeditor/
  repository kregexpeditor
  branch master
  min revision 409209
  max revision 753953
end match

match /trunk/playground/utils/kregexpeditor/
  repository kregexpeditor
  min revision 753954
  max revision 1281256
  branch master
end match

match /trunk/kdeutils/doc/KRegExpEditor/
  repository kregexpeditor
  min revision 119232
  max revision 409202
  branch master
  action recurse
  prefix doc/
end match

match /trunk/KDE/kdeutils/doc/KRegExpEditor/
  repository kregexpeditor
  min revision 409203
  max revision 753953
  branch master
  action recurse
  prefix doc/
end match

match /trunk/playground/utils/doc/KRegExpEditor/
  repository kregexpeditor
  min revision 753956
  max revision 1160903
  branch master
  action recurse
  prefix doc/
end match

match /
end match
