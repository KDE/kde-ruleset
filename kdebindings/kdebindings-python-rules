# Created by Nicolas Alvarez <nicolas.alvarez@gmail.com>
#

create repository KDE/kdebindings-python
end repository

include common-ignores

match /trunk/(KDE/)?kdebindings/python/
    repository KDE/kdebindings-python
    branch master
end match

# note the questionmark! this matches both 'lib' and 'libs' (it was renamed)
match /trunk/koffice/libs?/kross/python/
    repository KDE/kdebindings-python
    branch master
end match

match /trunk/koffice/kexi/scriptingcore/python/
    repository KDE/kdebindings-python
    branch master
end match

match /trunk/koffice/kexi/scripting/python/
    repository KDE/kdebindings-python
    branch master
end match

#
# recursion
#
match /(branches|tags)/(KDE/)?([^/]+)/$
    action recurse
end match

match /(branches|tags)/(KDE/)?([^/]+)/kdebindings/$
    action recurse
end match

#
# branches
#

# svn

match /branches/KDE/([^/]+)A/kdebindings/python/
    repository KDE/kdebindings-python
    branch KDE/\1a
end match

match /branches/KDE/([^/]+)/kdebindings/python/
    repository KDE/kdebindings-python
    branch KDE/\1
end match

# cvs

match /branches/KDE_(\d+)_(\d+)_BRANCH/kdebindings/python/
  repository KDE/kdebindings-python
  branch KDE/\1.\2
  max revision 409202
end match

#
# tags
#

# cvs time

match /tags/KDE_(\d+)_(\d+)_RELEASE/kdebindings/python/
  repository KDE/kdebindings-python
  branch refs/tags/v\1.\2.0
  max revision 409202
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_RELEASE/kdebindings/python/
  repository KDE/kdebindings-python
  branch refs/tags/v\1.\2.0
  max revision 85028
  min revision 68154
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)_RELEASE/kdebindings/python/
  repository KDE/kdebindings-python
  branch refs/tags/v\1.\2.\3
  max revision 409202
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)[Aa]_RELEASE/kdebindings/python/
  repository KDE/kdebindings-python
  branch refs/tags/v\1.\2.\3a
  min revision 192719
  max revision 409202
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)_BETA_(\d+)/kdebindings/python/
  repository KDE/kdebindings-python
  branch refs/tags/v\1.\2.\3-beta\4
  min revision 376088
  max revision 409202
  annotated true
end match

# svn time

match /tags/KDE/3.5.0-alpha1/kdebindings/python/
  repository KDE/kdebindings-python
  branch refs/tags/v3.4.90
  annotated true
end match

match /tags/KDE/3.5.0-beta1/kdebindings/python/
  repository KDE/kdebindings-python
  branch refs/tags/v3.4.91
  annotated true
end match

match /tags/KDE/([^/]+)A/kdebindings/python/
    repository KDE/kdebindings-python
    branch refs/tags/v\1a
    annotated true
end match

match /tags/KDE/([^/]+)/kdebindings/python/
    repository KDE/kdebindings-python
    branch refs/tags/v\1
    annotated true
end match

#
# ignore everything else
#
match /
end match
