# Created by Trever Fischer <tdfischer@fedoraproject.org>
#

create repository kscd
end repository

match /trunk/(KDE/)?kdemultimedia/kscd/
    repository kscd
    branch master
    min revision 309
end match

match /trunk/(KDE/)?kdemultimedia/docs?/kscd/
	repository kscd
	branch master
	prefix doc/
end match

match /tags/KDE/(\d+\.\d+\.\d+[aA]?)/kdemultimedia/kscd/
	repository kscd
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/(4\.5\.3-test)/kdemultimedia/kscd/
	repository kscd
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/(\d+\.\d+)/kdemultimedia/kscd/
	repository kscd
	branch refs/tags/v\1
	annotated true
end match

# create branches in each sub-repository
match /branches/KDE/(\d\.\d)/kdemultimedia/kscd/
	branch KDE/\1
	repository kscd
end match
