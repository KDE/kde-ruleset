    repository ${REPO}
    prefix ${PREFIX|/}
    branch work/personal/\1/\2
end match
match /branches/kdepim/([^~][^/]+)/kdemultimedia/${MODULE}/
    repository ${REPO}
    prefix ${PREFIX|/}
    branch \1
end match

