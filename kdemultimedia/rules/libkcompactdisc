create repository libkcompactdisc
end repository

match /trunk/(KDE/)?kdemultimedia/libkcompactdisc/
    repository libkcompactdisc
    branch master
    min revision 608874
end match

match /tags/KDE/([0-9]+\.[0-9]+\.[0-9]+[aA]?)/kdemultimedia/libkcompactdisc/
	repository libkcompactdisc
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/(4\.5\.3-test)/kdemultimedia/libkcompactdisc/
	repository libkcompactdisc
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/([0-9]+\.[0-9]+)/kdemultimedia/libkcompactdisc/
	repository libkcompactdisc
	branch refs/tags/v\1
	annotated true
end match

# create branches in each sub-repository
match /branches/KDE/([0-9]\.[0-9])/kdemultimedia/libkcompactdisc/$
	branch KDE/\1
	repository libkcompactdisc
end match
