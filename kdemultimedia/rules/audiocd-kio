#

create repository audiocd-kio
end repository

match /trunk/(KDE/)?kdemultimedia/kioslave/audiocd/
    repository audiocd-kio
    branch master
    min revision 157422
end match

match /trunk/kdebase/kioslave/audiocd/
	repository audiocd-kio
	branch master
	max revision 157421
	min revision 71925
end match

match /trunk/(KDE/)?kdemultimedia/docs?/kioslave/audiocd/
	repository audiocd-kio
	branch master
	prefix doc/
end match

match /tags/KDE/([0-9]+\.[0-9]+\.[0-9]+[aA]?)/kdemultimedia/kioslave/audiocd/
	repository audiocd-kio
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/(4\.5\.3-test)/kdemultimedia/kioslave/audiocd/
	repository audiocd-kio
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/([0-9]+\.[0-9]+)/kdemultimedia/kioslave/audiocd/
	repository audiocd-kio
	branch refs/tags/v\1
	annotated true
end match

# create branches in each sub-repository
match /branches/KDE/([0-9]\.[0-9])/kdemultimedia/kioslave/audiocd/$
	branch KDE/\1
	repository audiocd-kio
end match
