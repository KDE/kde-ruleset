create repository docs
end repository

match /trunk/(KDE/)?kdemultimedia/docs?/
    repository docs
    branch master
end match

match /trunk/kdereview/dragonplayer/doc/
	repository docs
	branch dragon-history
end match

match /trunk/playground/multimedia/videoplayer/doc/dragonplayer/
	repository docs
	branch dragon-history
end match

match /trunk/playground/multimedia/videoplayer/doc/en/
	repository docs
	branch dragon-history
end match

