create repository kmix
end repository

match /trunk/(KDE/)?kdemultimedia/kmix/
    repository kmix
    branch oldkmix1
    max revision 50715 # data got deleted in 50716
end match

match /trunk/(KDE/)?kdemultimedia/kmix/
    repository kmix
    branch master
    min revision 52199
end match

match /trunk/(KDE/)?kdemultimedia/kmix2/
    repository kmix
    branch kmix2
    max revision 52201 # dir got deleted in 52202
end match

match /trunk/(KDE/)?kdemultimedia/docs?/kmix/
	repository kmix
	branch master
	prefix doc/
end match

match /tags/KDE/(\d+\.\d+\.\d+[aA]?)/kdemultimedia/kmix/
	repository kmix
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/(4\.5\.3-test)/kdemultimedia/kmix/
	repository kmix
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/(\d+\.\d+)/kdemultimedia/kmix/
	repository kmix
	branch refs/tags/v\1
	annotated true
end match

# create branches in each sub-repository
match /branches/KDE/(\d\.\d)/kdemultimedia/kmix/
	branch KDE/\1
	repository kmix
end match
