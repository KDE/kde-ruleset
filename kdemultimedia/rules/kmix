declare REPO=kmix
declare MODULE=kmix

create repository ${REPO}
end repository

include kdemm-common

match /trunk/(KDE/)?kdemultimedia/${MODULE}/
    repository ${REPO}
    branch oldkmix1
    max revision 50715 # data got deleted in 50716
end match

match /trunk/(KDE/)?kdemultimedia/kmix2/
    repository ${REPO}
    branch kmix2
    max revision 52201 # dir got deleted in 52202
end match
match /
end match
