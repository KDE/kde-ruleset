#

create repository libkcddb
end repository

match /trunk/(KDE/)?kdemultimedia/libkcddb/
    repository libkcddb
    branch master
    min revision 137020
end match

match /trunk/(KDE/)?kdemultimedia/docs?/kcontrol/cddbretrieval/
	repository libkcddb
	branch master
	prefix kcmcddb/doc/
end match

match /tags/KDE/([0-9]+\.[0-9]+\.[0-9]+[aA]?)/kdemultimedia/libkcddb/
	repository libkcddb
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/(4\.5\.3-test)/kdemultimedia/libkcddb/
	repository libkcddb
	branch refs/tags/v\1
	annotated true
end match

match /tags/KDE/([0-9]+\.[0-9]+)/kdemultimedia/libkcddb/
	repository libkcddb
	branch refs/tags/v\1
	annotated true
end match

# create branches in each sub-repository
match /branches/KDE/([0-9]\.[0-9])/kdemultimedia/libkcddb/$
	branch KDE/\1
	repository libkcddb
end match
