create repository KDE/kdebindings-ruby
end repository


match /trunk/(KDE/)?kdebindings/qtruby/
    repository KDE/kdebindings-ruby
    branch master
    prefix qtruby/
    # qtruby was moved from / to /ruby/ in 653117 - so skip that commit to not match the deletion of the dir
    max revision 653116
end match

match /trunk/(KDE/)?kdebindings/korundum/
    repository KDE/kdebindings-ruby
    branch master
    prefix korundum/
    # korundum was moved from / to /ruby/ in 653122 - so skip that commit to not match the deletion of the dir
    max revision 653121
end match

match /trunk/(KDE/)?kdebindings/ruby/
    repository KDE/kdebindings-ruby
    branch master
    min revision 646834
end match

#
# recursion
#
match /(branches|tags)/KDE/([^/]+)/$
    action recurse
end match

match /(branches|tags)/KDE/([^/]+)/kdebindings/$
    action recurse
end match

# recurse when qtruby and korundum where moved
match /trunk/KDE/kdebindings/ruby/(qtruby|korundum)/$
    action recurse
end match

#
# branches
#

# svn

match /branches/KDE/([^/]+)A/kdebindings/(qtruby|korundum)/
    repository KDE/kdebindings-ruby
    prefix \2/
    branch KDE/\1a
end match

match /branches/KDE/([^/]+)A/kdebindings/ruby/
    repository KDE/kdebindings-ruby
    branch KDE/\1a
end match

match /branches/KDE/([^/]+)/kdebindings/(qtruby|korundum)/
    repository KDE/kdebindings-ruby
    prefix \2/
    branch KDE/\1
end match

match /branches/KDE/([^/]+)/kdebindings/ruby/
    repository KDE/kdebindings-ruby
    branch KDE/\1
end match

# cvs

# this commit is messed up - ignore it
match /branches/KDE_3_4_BRANCH/kdebindings/
    min revision 400296
    max revision 400296
end match

match /branches/KDE_(\d+)_(\d+)_BRANCH/kdebindings/(qtruby|korundum)/
  repository KDE/kdebindings-ruby
  prefix \3/
  branch KDE/\1.\2
  max revision 409202
end match

