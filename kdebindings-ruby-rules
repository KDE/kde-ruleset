# Created by Arno Rehn <arno@arnorehn.de>
#

create repository KDE/qtruby
end repository

create repository KDE/korundum
end repository

match /trunk/(KDE/)?kdebindings/(qtruby|ruby/q\w+)/
    repository KDE/qtruby
    branch master
end match

match /trunk/(KDE/)?kdebindings/(korundum|ruby/k\w+)/
    repository KDE/korundum
    branch master
end match

#
# recursion
#
match /branches/KDE/([^/]+)/$
    action recurse
end match

# I don't know if the following recursions are really needed.. but just to be sure I put them here.
match /trunk/KDE/kdebindings/ruby/$
    action recurse
end match

match /trunk/KDE/kdebindings/ruby/(qtruby|korundum)/(modules/)?$
    action recurse
end match

#
# branches
#

# svn

match /branches/KDE/([^/]+)A/kdebindings/(qtruby|ruby/q\w+)/
    repository KDE/qtruby
    branch KDE/\1a
end match

match /branches/KDE/([^/]+)/kdebindings/(korundum|ruby/k\w+)/
    repository KDE/korundum
    branch KDE/\1
end match

# cvs

match /branches/KDE_(\d+)_(\d+)_BRANCH/kdebindings/(qtruby|korundum)/
  repository KDE/\3
  branch KDE/\1.\2
  max revision 409202
end match

#
# tags
#

# cvs time

match /tags/KDE_(\d+)_(\d+)_RELEASE/kdebindings/(qtruby|korundum)/
  repository KDE/\3
  branch refs/tags/v\1.\2.0
  max revision 409202
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_RELEASE/kdebindings/(qtruby|korundum)/
  repository KDE/\3
  branch refs/tags/v\1.\2.0
  max revision 85028
  min revision 68154
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)_RELEASE/kdebindings/(qtruby|korundum)/
  repository KDE/\4
  branch refs/tags/v\1.\2.\3
  max revision 409202
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)[Aa]_RELEASE/kdebindings/(qtruby|korundum)/
  repository KDE/\4
  branch refs/tags/v\1.\2.\3a
  min revision 192719
  max revision 409202
  annotated true
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)_BETA_(\d+)/kdebindings/(qtruby|korundum)/
  repository KDE/\5
  branch refs/tags/v\1.\2.\3-beta\4
  min revision 376088
  max revision 409202
  annotated true
end match

# svn time

match /tags/KDE/([^/]+)A/kdebindings/(qtruby|ruby/q\w+)/
    repository KDE/qtruby
    branch refs/tags/v\1a
    annotated true
end match

match /tags/KDE/([^/]+)A/kdebindings/(korundum|ruby/k\w+)/
    repository KDE/korundum
    branch refs/tags/v\1a
    annotated true
end match

match /tags/KDE/([^/]+)/kdebindings/(qtruby|ruby/q\w+)/
    repository KDE/qtruby
    branch refs/tags/v\1
    annotated true
end match

match /tags/KDE/([^/]+)/kdebindings/(korundum|ruby/k\w+)/
    repository KDE/korundum
    branch refs/tags/v\1
    annotated true
end match

match /tags/KDE/3.5.0-alpha1/kdebindings/qtruby/
    repository KDE/qtruby
    branch refs/tags/v3.4.90
    min revision 443441
    max revision 445561
    annotated true
end match

match /tags/KDE/3.5.0-alpha1/kdebindings/korundum/
    repository KDE/korundum
    branch refs/tags/v3.4.90
    min revision 443441
    max revision 445561
    annotated true
end match

match /tags/KDE/3.5.0-beta1/kdebindings/qtruby/
    repository KDE/qtruby
    branch refs/tags/v3.4.91
    min revision 459298
    max revision 460260
    annotated true
end match

match /tags/KDE/3.5.0-beta1/kdebindings/korundum/
    repository KDE/korundum
    branch refs/tags/v3.4.91
    min revision 459298
    max revision 460260
    annotated true
end match

#
# ignore everything else
#
match /
end match
