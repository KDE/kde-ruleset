##############
# Branches...
##############
# Non merged branches
match /branches/work/(akonadi-ports)/(kdepimlibs)/
    repository KDE/\2
    branch \1
end match

# KDAB
match /branches/kdepim/(enterprise)/(kdepimlibs)/
    repository KDE/\2
    branch \1
end match
match /branches/kdepim/(enterprise4)/(kdepimlibs)/
    repository KDE/\2
    branch \1
end match
match /branches/work/(komo)/(kdepimlibs)/
    repository KDE/\2
    branch \1
end match

match /branches/KDE/(\d+)\.(\d+)/(kdepimlibs)/
    repository KDE/\3
    branch \1.\2
end match

##############
# Tags
##############
match /tags/KDE/(\d+)\.(\d+)/(kdepimlibs)/
    repository KDE/\3
    branch refs/tags/\1.\2
    annotated true
end match
match /tags/KDE/(\d+)\.(\d+)\.(\d+)/(kdepimlibs)/
    repository KDE/\4
    branch refs/tags/\1.\2.\3
    annotated true
end match

##############
# Ignore everything else
##############
match /
end match
