#    branch master
#    min revision 788161
#end match
match /trunk/KDE/kdepim/akonadi/libakonadi/
    repository KDE/kdepimlibs
    branch master
    rootdir /trunk/KDE/kdepim/akonadi/
    max revision 788161
    min revision 559105
end match
match /trunk/KDE/kdepim/libakonadi/
    repository KDE/kdepimlibs
    rootdir /trunk/KDE/kdepim/
    branch master
    max revision 559105
end match

