##############
# Branches...
##############
match /branches/KDE_(\d+)_(\d+)_BRANCH/
    repository KDE/kdepim
    branch \1.\2
end match
match /branches/KDE/(\d+)\.(\d+)/kdepim/
    repository KDE/kdepim
    branch \1.\2
end match
match /branches/(work)/~([^/]+)/([^/]*kdepim[^/]*)/
    repository KDE/kdepim
    branch \1/personal/\2
end match
#match /branches/kdepim/([^/]+)/
#    repository KDE/kdepim
#    branch \1
#end match
#match /branches/(.+)/[^/]*kdepim[^/]*/
#    repository KDE/kdepim
#    branch \1
#end match

match /branches/work/(akonadi-ports)/(kdepim)/
    repository KDE/\2
    branch \1
end match
match /branches/kdepim/(enterprise|enterprise4)/(kdepim)/
    repository KDE/\2
    branch \1
end match
match /branches/work/(komo)/(kdepim)/
    repository KDE/\2
    branch \1
end match
match /branches/work/(kdepim)-(3.5.5\+)/
    repository KDE/\1
    branch \1-\2
end match

##############
# Tags
##############
# Should be regenerated from cvs directly into git
#match /tags/KDE_(\d+)_(\d+)_(\d+[a-zA-Z]?)_RELEASE/(kdepim)/
#    repository KDE/\4
#    branch tags/\1.\2.\3
#end match
match /tags/KDE/(\d+)\.(\d+)/(kdepim)/
    repository KDE/\3
    branch refs/tags/\1.\2
    annotated true
end match
match /tags/KDE/(\d+)\.(\d+)\.(\d+)/(kdepim)/
    repository KDE/\4
    branch refs/tags/\1.\2.\3
    annotated true
end match

##############
# Ignore everything else
##############
match /
end match
