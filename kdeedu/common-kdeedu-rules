# these are needed to match rules in eg. kdebase/apps

match /tags/KDE/4.4.85/
   min revision 1135916
end match

match /(branches|tags)/[^/]+/kdeedu/$
  action recurse
end match
match /(branches|tags)/KDE/[^/]+/kdeedu/$
  action recurse
end match

match /trunk/(KDE/)?kdeedu/doc/${MODULE}/
  repository ${REPO}
  prefix doc/
  branch master
end match