create repository edu-www
end repository

# This is mainly for performance reasons, since we'll filter-branch the files out anyway.
match /trunk/www/(sites|areas)/edu/(new/)?(kstars|klettres)/downloads/[^/]+\.(bz2|gz)$
end match
match /trunk/www/sites/edu/marble/newstuff/maps/[^/]+\.(tar\.gz|zip)$
end match
match /trunk/www/sites/edu/marble/downloads/.+\.(exe|zip|deb)$
end match

match /trunk/www/sites/edu/old/
end match

match /trunk/www/areas/edu/
    repository edu-www
    branch master
end match

match /trunk/www/sites/edu/new/
    repository edu-www
    branch new
    max revision 759748
end match
match /trunk/www/sites/edu/
    repository edu-www
    branch master
end match

match /trunk/www/sites/www-devel/edu/
    repository edu-www
    branch devel
end match

match /
end match
