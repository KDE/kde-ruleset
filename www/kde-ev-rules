create repository kde-ev-www
end repository

# this won't actually appear as an 'old' branch in the finished
# git repo, since this path was deleted in r210114
match /trunk/www/kde-ev/
    repository kde-ev-www
    branch old
end match

match /trunk/www/areas/kde-ev/
    repository kde-ev-www
    branch master
end match

match /trunk/www/sites/kde-ev/
    repository kde-ev-www
    branch master
end match

match /trunk/www/sites/www-devel/kde-ev/
    repository kde-ev-www
    branch devel
end match

match /
end match
