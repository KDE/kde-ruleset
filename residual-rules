
# debugging rules
create repository residual-${REPO}
end repository

match /.*/kde-i18n/
end match
match /.*/l10n/
end match
match /.*/l10n-kde4/
end match
match /.*\.pot?$
end match
match /trunk/kdeaccessibility/IconThemes/
end match
match /trunk/(KDE/)?kdeartwork/
end match

match /(.*${REPO}.*)$
    repository residual-${REPO}
    branch master
    prefix \1
end match
