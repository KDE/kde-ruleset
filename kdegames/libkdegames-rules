create repository libkdegames
end repository

include ../common-kde-ignores

declare REPO=libkdegames

match /trunk/KDE/kdegames/libkdegames/
    repository libkdegames
    branch master
end match

# BEGIN version branches
match /(branches|tags)/KDE_\d+[^/]+/(kdegames/)?$
  action recurse
end match
match /(branches|tags)/KDE/\d+[^/]+/(kdegames/)?$
  action recurse
end match

match /branches/KDE_(\d+)_(\d+)_BRANCH/kdegames/libkdegames/
    repository libkdegames
    branch KDE/\1.\2
end match
match /branches/KDE/(\d+)\.(\d+)/kdegames/libkdegames/
    repository libkdegames
    branch KDE/\1.\2
end match
# END

# BEGIN release tags
match /tags/KDE_(\d+)_(\d+)_RELEASE/kdegames/libkdegames/
    repository libkdegames
    branch refs/tags/v\1.\2.0
    annotated true
end match
match /tags/KDE_(\d+)_(\d+)_(\d+)_RELEASE/kdegames/libkdegames/
    repository libkdegames
    branch refs/tags/v\1.\2.\3
    annotated true
end match
match /tags/KDE/(\d+)\.(\d+)/kdegames/libkdegames/
    repository libkdegames
    branch refs/tags/v\1.\2.0
    annotated true
end match
match /tags/KDE/(\d+)\.(\d+)\.(\d+)/kdegames/libkdegames/
    repository libkdegames
    branch refs/tags/v\1.\2.\3
    annotated true
end match
# END

declare MODULE_RECURSE=(kdegames/)?
declare MODULE=kdegames/libkdegames

# use all other feature branches
include ../feature-branches

include ../common-kde-tags-rules

match /
end match
