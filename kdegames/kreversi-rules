declare REPO=kreversi
include common-kdegames-init

match /branches/work/kreversi-rewrite/
    repository ${REPO}
    branch refs/workbranch/kreversi-rewrite
end match

# in early revisions (see r29151)
match /branches/KDE_1_1_BRANCH/kdegames/kreversi/html/
    repository ${REPO}
    branch master
    prefix doc/
end match
match /trunk/kdegames/kreversi/html/
    repository ${REPO}
    branch master
    prefix doc/
end match

include common-kdegames-end
