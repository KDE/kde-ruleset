create repository kpat
end repository

declare REPO=kpat

match /trunk/(KDE/)?kdegames/kpat/
    repository kpat
    branch master
end match

# BEGIN version branches
match /(branches|tags)/[^/]+/(kdegames/)?$
  action recurse
end match
match /(branches|tags)/KDE/[^/]+/(kdegames/)?$
  action recurse
end match

match /branches/KDE_(\d+)_(\d+)_BRANCH/kdegames/kpat/
    repository kpat
    branch \1.\2
end match
match /branches/KDE/(\d+)\.(\d+)/kdegames/kpat/
    repository kpat
    branch \1.\2
end match
# END

# BEGIN work branches
match /branches/work/kde4/kdegames/$
    action recurse
end match
match /branches/work/kde4/kdegames/kpat/
    repository kpat
    branch kde4
end match

match /branches/work/kpat_solvers/
    repository kpat
    branch kpat_solvers
end match

match /branches/work/kpat_65506/
    repository kpat
    branch kpat_65506
end match
# END

declare MODULE=kdegames/kpat
declare MODULE_RECURSE=(kdegames/)?
include ../feature-branches

match /
end match
