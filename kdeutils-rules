##### Create the KDE module #####
create repository KDE/kdeutils
end repository

##### Ignore/Delete rules #####
### Ignore the SVN-adjusting commits
match /
  min revision 409202
  max revision 409209
end match

##### Main rules #####
### trunk/kdeutils
match /trunk/kdeutils/
  repository KDE/kdeutils
  branch master
  max revision 409202
end match

### trunk/KDE/kdeutils
match /trunk/KDE/kdeutils/
  repository KDE/kdeutils
  branch master
  min revision 409209
end match

##### Branches #####
### CVS branches
match /branches/KDE_(\d+)_(\d+)_BRANCH/kdeutils/
  repository KDE/kdeutils
  branch KDE/\1.\2
  max revision 409202
end match

match /branches/KDE/([^/]+)A/kdeutils/
  repository KDE/kdeutils
  branch KDE/\1a
  min revision 409209
end match

### Release branches
match /branches/KDE/([^/]+)/kdeutils/
  repository KDE/kdeutils
  branch KDE/\1
  min revision 409209
end match

### Create branches in each sub-repository
match /branches/KDE/([^/]+)/$
  action recurse
  min revision 757128
end match

##### Tags #####
### CVS/old-style tags
match /tags/KDE_(\d+)_(\d+)_RELEASE/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v\1.\2.0
  annotated true
  min revision 68154
  max revision 85028
end match

match /tags/KDE_(\d+)_(\d+)_RELEASE/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v\1.\2.0
  annotated true
  max revision 409202
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)_RELEASE/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v\1.\2.\3
  annotated true
  max revision 409202
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)[Aa]_RELEASE/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v\1.\2.\3a
  annotated true
  min revision 192719
  max revision 409202
end match

match /tags/KDE_(\d+)_(\d+)_(\d+)_BETA_(\d+)/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v\1.\2.\3-beta\4
  annotated true
  min revision 376088
  max revision 409202
end match

### 3.5.0-alpha1
match /tags/KDE/3.5.0-alpha1/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v3.4.90
  min revision 443441
  max revision 445561
  annotated true
end match

### 3.5.0-beta1
match /tags/KDE/3.5.0-beta1/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v3.4.91
  min revision 459298
  max revision 460260
  annotated true
end match

### New-style tags
match /tags/KDE/([^/]+)A/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v\1a
  annotated true
end match

match /tags/KDE/([^/]+)/kdeutils/
  repository KDE/kdeutils
  branch refs/tags/v\1
  annotated true
end match

##### Application-specific rules #####
### Ark in branches/work/libarchive-based-ark/ark
match /branches/work/libarchive-based-ark/ark/
  repository KDE/kdeutils
  prefix ark/
  branch master
  min revision 672750
  max revision 694571
end match

### Filelight in playground
match /trunk/playground/utils/filelight/
  repository KDE/kdeutils
  prefix filelight/
  branch master
  # min revision 920168
  # max revision 1174508
end match

