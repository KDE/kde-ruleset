#!/bin/sh

svn export -q  svn://anonsvn.kde.org/home/kde/trunk/kde-common/accounts accounts.tmp

IFS="
"

echo -n > account-map

for person in `cat accounts.tmp`; do
	LINE=`echo $person | sed -e "s/\>[[:blank:]]*\</ /g;\
		s/[[:graph:]]*@[[:graph:]]*/<&>/" -`
	echo $LINE >> account-map
done

rm -f accounts.tmp
