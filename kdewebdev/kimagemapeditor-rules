create repository kimagemapeditor
end repository

include ../common-kde-ignores

declare REPO=kimagemapeditor
declare MODULE=kdewebdev/kimagemapeditor
declare MODULE_RECURSE=(kdewebdev/)?

# r745132 created a 3.97 tag for all of KDE, but r745137 deleted it for
# some parts of kdewebdev. It seems svn-all-fast-export doesn't like it
# when a tag is deleted, so let's ignore the commit that created it

match /tags/KDE/3.97/
    min revision 745132
    max revision 745132
end match

include kdewebdev-common-rules

# debugging rules
create repository residual-kimagemapeditor
end repository

match /.*/kde-i18n/
end match
match /.*/l10n/
end match
match /.*/l10n-kde4/
end match
match /.*\.pot?$
end match
match /trunk/kdeaccessibility/IconThemes/
end match
match /trunk/KDE/kdeartwork/
end match

match /(.*kimagemapeditor.*)$
    repository residual-kimagemapeditor
    branch master
    prefix \1
end match


match /
end match
