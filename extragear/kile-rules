# Ignore some upheaval in KDE's SVN history.
match /
    min revision 409202
    max revision 409210
end match
match /
    min revision 411973
    max revision 411973
end match

match /trunk/kdeextragear-2/kile/
    repository kile
    branch 2.0
end match

match /trunk/kdeextragear-2/doc/kile/
    repository kile
    prefix doc/
    branch 2.0
end match

