create repository kphotoalbum
end repository

match /trunk/extragear/graphics/kphotoalbum/
    repository kphotoalbum
    branch master
end match

match /branches/extragear/kde3/graphics/kphotoalbum/
    repository kphotoalbum
    branch master
end match

match /trunk/extragear/graphics/KimDaBa/
    repository kphotoalbum
    branch master
end match

