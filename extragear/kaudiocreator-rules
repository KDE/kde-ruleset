create repository kaudiocreator
end repository

match /trunk/(KDE/)?kdemultimedia/kaudiocreator/
    repository kaudiocreator
    branch master
end match
match /trunk/(KDE/)?kdemultimedia/doc/kaudiocreator/
    repository kaudiocreator
    branch master
    prefix doc/
end match

