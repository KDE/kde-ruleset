#
# Adapted from many examples by Robby Stephenson <robby@periapsis.org>

#
# Create the tellico repository
#
create repository tellico
end repository

# Start in playground
match /trunk/playground/office/tellico/
  repository tellico
  branch master
  min revision 927058
  max revision 965369
end match

match /trunk/playground/office/doc/tellico/
  repository tellico
  branch master
  prefix doc/
  min revision 927058
  max revision 965369
end match

# move to kdereview
match /trunk/kdereview/tellico/
  repository tellico
  branch master
  min revision 965369
  max revision 973887
end match

match /trunk/kdereview/doc/tellico/
  repository tellico
  branch master
  prefix doc/
  min revision 965369
  max revision 973887
end match

# move to extragear
match /trunk/extragear/office/tellico/
  repository tellico
  branch master
  min revision 973887
  max revision 1227610
end match

match /trunk/extragear/office/doc/tellico/
  repository tellico
  branch master
  prefix doc/
  min revision 973887
  max revision 1197135
end match

# Tags
match /tags/tellico/([^/]+)/
  repository tellico
  branch refs/tags/v\1
  annotated true
end match

# ignore everything else
match /
end match
